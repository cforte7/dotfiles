# Local Servers
alias run-dev="SQLA_BE_QUIET=1 python manage.py runserver --settings=sheik.settings_dev"
alias pbuild='LOCAL_STAGE=true npm run build:watch'
alias flyrep='./flyway-7.5.4/flyway -configFiles=conf/factory_dev.conf repair'
alias flymig='./flyway-7.5.4/flyway -configFiles=conf/factory_dev.conf migrate'alias local-shell='python manage.py shell --settings=sheik.settings_dev'

# Other
# interactively pick a log group, tail it, and forward args/options
function cwt () {
  cw tail $(cw ls groups | fzf) "$@"
}
